<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="by.pvt.predkel.entities.FlammableSubstance" table="FlammableSubstance" lazy="false">
        <id name="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="nameOfSubstance" type="java.lang.String"/>
        <property name="amountOfCombustionAir" type="java.lang.Double"/>
        <property name="combustionHeat" type="java.lang.Double"/>
        <property name="averageSpeedBurnout" type="java.lang.Double"/>
    </class>
</hibernate-mapping>