<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="by.pvt.predkel.entities.options.DetermineTheTypeOfFire" table="DetermineTheTypeOfFire">
        <id name="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="proemnostOfRoom" type="java.lang.Double"/>
        <property name="averageAmountOfCombustionAir" type="java.lang.Double"/>
        <property name="specificCriticalAmountOfFireLoad" type="java.lang.Double"/>
        <property name="specificValueOfFireLoad" type="java.lang.Double"/>
        <property name="prn" type="java.lang.Boolean"/>
    </class>
</hibernate-mapping>