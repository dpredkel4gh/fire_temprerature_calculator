<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="by.pvt.predkel.entities.Aperture" table="Aperture">
        <id name="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="typeOfAperture" type="java.lang.String"/>
        <property name="width" type="java.lang.Double"/>
        <property name="height" type="java.lang.Double"/>
        <property name="squareOfAperture" type="java.lang.Double"/>
        <property name="count" type="java.lang.Integer"/>

    </class>
</hibernate-mapping>