<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="by.pvt.predkel.entities.options.IntegratedThermalAndTechnicalParameters"
           table="IntegratedThermalAndTechnicalParameters">
        <id name="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="maximumMeanBulkTemperature" type="java.lang.Double"/>
        <property name="durationOfFireSurround" type="java.lang.Double"/>
        <property name="timeReachMaximumMeanBulkTemperature" type="java.lang.Double"/>
        <property name="maximumAverageTemperatureOfWallSurface" type="java.lang.Double"/>
        <property name="timeToReachMaximumTemperatureOfWallSurface" type="java.lang.Double"/>
        <property name="averageMaximumTemperatureOfSlab" type="java.lang.Double"/>
        <property name="timeToReachMaximumTemperatureOfSlabSurface" type="java.lang.Double"/>
    </class>
</hibernate-mapping>